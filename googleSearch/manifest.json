{
  "manifest_version": 3,
  "name": "Google Search Addon",
  "version": "1.0",
  "description": "Add Google search option to the right-click menu.",
  "permissions": ["contextMenus"],
  "icons": {
    "48": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
